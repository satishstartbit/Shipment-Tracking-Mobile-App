{
  "expo": {
    "name": "Shipment-Tracking-Mobile-App",
    "slug": "Shipment-Tracking-Mobile-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Campa_Logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.campaui.ShipmentTrackingMobileApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/background_color.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.campaui.ShipmentTrackingMobileApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/background_color.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9a3549ea-1f7a-4ce2-b05c-121e5cd5b7fb"
      }
    },
    "owner": "campaui"
  }
}
